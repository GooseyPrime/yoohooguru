{
  "projects": [
    {
      "displayName": "backend",
      "testMatch": ["<rootDir>/backend/tests/**/*.test.js"],
      "testEnvironment": "node",
      "setupFilesAfterEnv": ["<rootDir>/backend/jest.setup.js"],
      "testTimeout": 45000,
      "collectCoverageFrom": [
        "<rootDir>/backend/src/**/*.js",
        "!<rootDir>/backend/src/**/*.test.js"
      ]
    }
  ]
}