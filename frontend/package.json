    "file-loader": "^6.2.0",
    "url-loader": "^4.1.1",
    "jest": "^29.7.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^5.16.5",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.1.1",
    "workbox-webpack-plugin": "^7.0.0"
  },
  "keywords": [
    "react",
    "react-native-web",
    "skill-sharing",
    "pwa"
  ],
  "author": "GooseyPrime",
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
